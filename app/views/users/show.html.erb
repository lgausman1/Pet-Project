<% if current_user == @user %>
<div class="jumbotron">
<div class="container">
<h3>Hello, <%= @user.first_name.capitalize %></h3></div></div>
<% else %>
<div class="jumbotron">
<div class="container">
<h3><%= @user.first_name.capitalize %>'s page</h3></div></div>
<% end %>













<!-- logout button -->
<%= button_to "Logout", logout_path, class: "btn btn-primary", method: :delete %>