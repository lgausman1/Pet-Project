<% if current_user == @user %>
<div class="jumbotron"><h3>Hello, <%= @user.first_name.capitalize %></h3></div>
<% else %>
<div class="jumbotron"><%= @user.first_name.capitalize %>'s page</div>
<% end %>













<!-- logout button -->
<%= button_to "Logout", logout_path, class: "btn btn-primary", method: :delete %>